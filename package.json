{
  "name": "excore",
  "version": "0.0.0",
  "description": "A small dependency injection framework for Express. Inspired by .Net Core.",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf build && babel src --out-dir build",
    "watch": "rm -rf build && babel src --watch --out-dir build",
    "watch-sample": "rm -rf sample/basic/build && babel sample/basic/src --watch --out-dir sample/basic/build",
    "sample": "DEBUG=excore:* nodemon sample/basic/build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhaldkhein/excore.git"
  },
  "author": "RhaldKhein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhaldkhein/excore/issues"
  },
  "homepage": "https://github.com/rhaldkhein/excore#readme",
  "keywords": [
    "express",
    "modular",
    "dependency",
    "injection",
    "framework"
  ],
  "dependencies": {
    "callsite": "^1.0.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "jservice": "^0.1.18",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.get": "^4.4.2",
    "passport": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
